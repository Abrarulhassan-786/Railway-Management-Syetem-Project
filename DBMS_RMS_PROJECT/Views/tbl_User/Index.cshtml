@model IEnumerable<DBMS_RMS_PROJECT.Models.tbl_User>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.U_Fname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Lname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Mobile_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.U_Password)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.U_Fname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Lname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Mobile_No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.U_Password)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.U_Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.U_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.U_Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
